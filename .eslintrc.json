{
  "root": true,
  "extends": "airbnb",
  "parserOptions": {
    "sourceType": "script"
  },
  "rules": {
    "no-underscore-dangle": "off",
    "func-names": "off",
    "comma-dangle": "off",
    "no-use-before-define": ["error", "nofunc"],
    "no-prototype-builtins": "off",
    "strict": ["error", "safe"],
    "no-console": "off",
    "prefer-rest-params": "off",
    "max-len": "off",
    "linebreak-style": "off",
    "prefer-template": "warn",
    "prefer-arrow-callback": "warn",
    "eol-last": "off"
  }
}
